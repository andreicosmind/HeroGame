class Hero:
	def __init__(self, health = 100, attack = 5):
		self.health = health
		self.attack = attack
		
def heroFight(Hero1, Hero2):
	Hero2.health -=Hero1.attack
	if Hero1.health <= 0 or Hero2.health <=0: return Hero1, Hero2
	else:
		Hero1.health-=Hero2.attack
		return Hero1, Hero2
Diaconu = Hero()
Oprisa = Hero()
while Diaconu.health >0 and Oprisa.health > 0:
	heroFight(Diaconu, Oprisa)
print (Diaconu.health)
print (Oprisa.health)


print("muie")